<?xml version="1.0" encoding="utf-8"?>
<type name="ConditionCollection" fullname="Fields.Skyrim.ConditionCollection" category="Fields (Skyrim)" gametitle="Skyrim">
  <summary>
            Represents a collection of <see cref="Fields.Skyrim.Condition">Conditions</see>.
            </summary>
  <property name="Count" gametitle="Skyrim">
    <signature>
      <c>int</c> Count</signature>
    <summary>
            Gets the number of <see cref="Fields.Skyrim.Condition">Conditions</see> in this collection.
            </summary>
  </property>
  <method name="Add" extension="false" gametitle="Skyrim">
    <signature>
      <c>void</c> Add(<see cref="Fields.Skyrim.Condition" />)</signature>
    <summary>
            Adds a <see cref="Fields.Skyrim.Condition" /> to this collection.
            </summary>
  </method>
  <method name="Clear" extension="false" gametitle="Skyrim">
    <signature>
      <c>void</c> Clear()</signature>
    <summary>
            Removes all <see cref="Fields.Skyrim.Condition">Conditions</see> from this collection.
            </summary>
  </method>
  <method name="Remove" extension="false" gametitle="Skyrim">
    <signature>
      <c>void</c> Remove(<see cref="Fields.Skyrim.Condition" />)</signature>
    <summary>
            Removes a <see cref="Fields.Skyrim.Condition" /> from this collection.
            </summary>
  </method>
  <remarks>
            Use any of the <see cref="Helpers.Skyrim.Functions" /> helper methods to create new conditions.
            Conditions can be removed from a collection using the <code>Remove()</code> method during a <c>foreach</c> iteration.
            </remarks>
</type>